/* pages/home/search.wxss */

.bg{
  position: absolute;
  z-index: -1;
  width: 100vw;
  height: 100vh;
  filter: blur(20px)
}
.container{

}
.search-view{
  padding: 0 50rpx;
  color: #fff;
}
.search-form{
  display: block;
  padding: 0 10rpx;
  overflow: hidden;
  border: 1px solid #fff;
  border-radius: 10rpx;
}
.search-form button{
  float: right;
  margin-left: 10rpx;
  width: 70rpx;
  height: 70rpx;
  background: transparent;
}
.search-form button::after{
  border: none;
}
.search-form image{
  display: block;
  width: 100%;
  height: 100%;
}
.search-form input{
  padding: 10rpx 0;
}
.logo{
  display: block;
  padding-top: 150rpx;
  padding-bottom: 35rpx;
  text-align: center;
  font-size: 60rpx;
}
.like{
  display: flex;
  flex-wrap: wrap;
  padding-top: 10rpx;
}
.like navigator{
  text-align: center;
  width: 25%;
  padding: 6rpx 0;
}